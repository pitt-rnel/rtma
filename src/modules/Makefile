MAKEFILE =	Makefile
QMAKE    =	qmake
DEL_FILE =    rm -f
CHK_DIR_EXISTS= test -d
MKDIR    = mkdir -p
INSTALL_FILE= 
INSTALL_DIR = 
SUBTARGETS =	 \
		sub-MessageManager \
		sub-QuickLogger

first: all

all: Makefile $(SUBTARGETS)

MessageManager/$(MAKEFILE): 
	@$(CHK_DIR_EXISTS) "MessageManager" || $(MKDIR) "MessageManager"
	cd MessageManager && $(QMAKE) MessageManager.pro -o $(MAKEFILE)
sub-MessageManager: MessageManager/$(MAKEFILE) FORCE
	cd MessageManager && $(MAKE) -f $(MAKEFILE)

QuickLogger/$(MAKEFILE): 
	@$(CHK_DIR_EXISTS) "QuickLogger" || $(MKDIR) "QuickLogger"
	cd QuickLogger && $(QMAKE) QuickLogger.pro -o $(MAKEFILE)
sub-QuickLogger: QuickLogger/$(MAKEFILE) FORCE
	cd QuickLogger && $(MAKE) -f $(MAKEFILE)

Makefile: Modules.pro  /usr/share/qt4/mkspecs/default/qmake.conf 
	$(QMAKE) -o Makefile Modules.pro
qmake: qmake_all
	@$(QMAKE) -o Makefile Modules.pro

all: $(SUBTARGETS)
qmake_all: MessageManager/$(MAKEFILE) QuickLogger/$(MAKEFILE)
	 ( [ -d MessageManager ] && cd MessageManager ; grep "^qmake_all:" $(MAKEFILE) && $(MAKE) -f $(MAKEFILE) qmake_all; ) || true
	 ( [ -d QuickLogger ] && cd QuickLogger ; grep "^qmake_all:" $(MAKEFILE) && $(MAKE) -f $(MAKEFILE) qmake_all; ) || true
clean uicables mocables uiclean mocclean lexclean yaccclean : qmake_all FORCE
	 ( [ -d MessageManager ] && cd MessageManager ; $(MAKE) -f $(MAKEFILE) $@; ) || true
	 ( [ -d QuickLogger ] && cd QuickLogger ; $(MAKE) -f $(MAKEFILE) $@; ) || true
uninstall_subdirs: qmake_all FORCE
	 ( [ -d MessageManager ] && cd MessageManager ; $(MAKE) -f $(MAKEFILE) uninstall; ) || true
	 ( [ -d QuickLogger ] && cd QuickLogger ; $(MAKE) -f $(MAKEFILE) uninstall; ) || true
install_subdirs: qmake_all FORCE
	 ( [ -d MessageManager ] && cd MessageManager ; $(MAKE) -f $(MAKEFILE) install; ) || true
	 ( [ -d QuickLogger ] && cd QuickLogger ; $(MAKE) -f $(MAKEFILE) install; ) || true
distclean: qmake_all FORCE
	 ( [ -d MessageManager ] && cd MessageManager ; $(MAKE) -f $(MAKEFILE) $@; $(DEL_FILE) $(MAKEFILE); ) || true
	 ( [ -d QuickLogger ] && cd QuickLogger ; $(MAKE) -f $(MAKEFILE) $@; $(DEL_FILE) $(MAKEFILE); ) || true

install:  install_subdirs

uninstall:  uninstall_subdirs

